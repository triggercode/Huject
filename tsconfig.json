{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "preserveConstEnums": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "strict": true,
    "noImplicitAny": false,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}